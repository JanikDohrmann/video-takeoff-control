<Window x:Class="video_takeoff_control.CompetitionNameModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:video_takeoff_control"
        mc:Ignorable="d"
        Title="Change Competition Name" Height="100" Width="300">
    <WrapPanel Margin="0,10,0,0" Background="LightGray">
        <TextBlock Text="Wettkampfname:" Margin="8,0,5,0"/>
        <TextBox Name="textCompetitionName" Text="Wettkampfname" Width="200" Margin="5,0,5,0"/>
        <Button x:Name="saveCompetitioNameButton" Content="Save" Click="saveCompetitionName_Click"/>
    </WrapPanel>
</Window>
